@model NHH.Models.Contract.ContractUnitSpecInfo

<h4 class="lighter">商户责任</h4>
@if (Model.ContractStatus == 2)
{
    <a class="edit-btn" href="javascript:void(0);" id="lbtnUnitSpec2" data-contractid="@Model.ContractID">编辑信息</a>
    <script type="text/javascript">
        $("#lbtnUnitSpec2").click(function () {
            var url = "@Url.Action("UnitSpec2", "ContractMgmt", new { area = "Contract" })";
            url += "?contractId=" + $("#ContractID").val();
            $("#unitSpec2").load(url);
        });
    </script>
}
<dl class="form-content textstyle">
    <dd>
        <label>地面：</label>
        <span>@Model.Floor</span>
    </dd>
    <dd>
        <label>顶面：</label>
        <span>@Model.Ceiling</span>
    </dd>
    <dd>
        <label>墙：</label>
        <span>@Model.Wall</span>
    </dd>
    <dd>
        <label>柱：</label>
        <span>@Model.Pillar</span>
    </dd>
    <dd>
        <label>楼板承重：</label>
        <span>@Model.FloorBearing</span>
    </dd>
    <dd>
        <label>给水：</label>
        <span>@Model.WaterSupply</span>
    </dd>
    <dd>
        <label>排水：</label>
        <span>@Model.WaterDrain</span>
    </dd>
    <dd>
        <label>店铺门面：</label>
        <span>@Model.Door</span>
    </dd>
    <dd>
        <label>标牌：</label>
        <span>@Model.Logo</span>
    </dd>
    <dd>
        <label>供电系统：</label>
        <span>@Model.ElectricityUsage</span>
    </dd>
    <dd>
        <label>消防系统：</label>
        <span>@Model.FireProtection</span>
    </dd>
    <dd>
        <label>广播系统：</label>
        <span>@Model.Broadcasting</span>
    </dd>
    <dd>
        <label>空调系统：</label>
        <span>@Model.AirCondition</span>
    </dd>
    <dd>
        <label>排油烟系统：</label>
        <span>@Model.Smoke</span>
    </dd>
    <dd>
        <label>安防系统：</label>
        <span>@Model.Security</span>
    </dd>
    <dd>
        <label>综合布线：</label>
        <span>@Model.Wiring</span>
    </dd>
    <dd>
        <label>煤气：</label>
        <span>@Model.Gas</span>
    </dd>
</dl>
<div class="clear"></div>