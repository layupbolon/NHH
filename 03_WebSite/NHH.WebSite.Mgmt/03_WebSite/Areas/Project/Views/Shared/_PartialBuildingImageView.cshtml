@model List<string>
<link rel="stylesheet" href="~/Content/indexImge.css" type="text/css" />
@Html.AntiForgeryToken()
@Html.ValidationSummary(true)
<div class="modal-dialog">
    <div class="modal-content">
        <div class="modal-header no-padding">
            <div class="table-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
                    <span class="white">×</span>
                </button>
                楼宇图片展示
            </div>
        </div>

        <div class="modal-body no-padding nothing">
            <div class="popWinStyle popWin-form" id="slides">
                <ul style="visibility:visible;" id="picShow">
                    @{
                        if (Model.Count > 0)
                        {
                            @*for (int i = 0; i < Model.Count; i++)
                        {
                            <li >
                             <img src="@Model[i]" width="190" height="190" />
                            </li>
                        }*@
                            string tmpSrc = Model.FirstOrDefault().Replace(",", "");
                            <li>
                                <img src="@NHH.Framework.Utility.ParamManager.GetStringValue("ImageSite")@tmpSrc" width="400" height="400" />
                            </li>
                        }
                        else
                        {
                            <li style="height:220px;">
                                @*<label for="inputWarning" class="col-xs-12 col-sm-3 control-label no-padding-right">该楼宇没有图片可用显示</label>*@
                                <span style="color:brown;font-size:18px;">该楼层次没有图片可用显示</span>
                            </li>
                        }
                    }
                </ul>
                @*<a href="javascript:;" id="prev" class="arrow"><img src="~/Content/images/index-Pre.png" /><span>3/1</span></a>
                <a href="javascript:;" id="next" class="arrow"><img src="~/Content/images/index-Next.png" /></a>*@
            </div>
            @*<input type="button" value="保存" class="btn btn-xs btn-success btn-font btnSubmit" style="margin-left:50%;margin-bottom: 20px;" />*@
            @*<div class="ui-btngroup">
                <a href="javascript:void(0)" class="btn btn-xs btn-success btn-font mr20 btnSubmit" id="CloseImageShow">关闭</a>
            </div>*@


        </div>
    </div><!-- /.modal-content -->
</div><!-- /.modal-dialog -->

<script language="javascript" src="@Url.Content("~/Scripts/indexImge.js")" type="text/javascript"></script>
<script type="text/javascript">

    $(function () {
        //先提交表单信息
        $("#CloseImageShow").click(function (e) {
            $("#modal").html("").modal();
        });
        $("#btnCancel").click(function (e) {

        });
    });

</script>


